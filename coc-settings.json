{
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    },
    "clangd": {
      "command": "clangd",
      "args": ["--background-index"],
      "rootPatterns": ["compile_flags.txt", "compile_commands.json", ".vim/", ".git/", ".hg/"],
      "filetypes": ["c", "cpp", "objc", "objcpp"]
    },
    "dart": {
      "command": "/home/fujino/git/flutter/bin/cache/dart-sdk/bin/dart",
      "args": ["/home/fujino/git/flutter/bin/cache/dart-sdk/bin/snapshots/analysis_server.dart.snapshot", "--lsp"],
      "filetypes": ["dart"]
    },
    "golang": {
      "command": "gopls",
      "args": ["serve", "-debug", "0.0.0.0:8484", "-rpc.trace"],
      "disableWorkspaceFolders": true,
      "filetypes": ["go"],
      "rootPatterns": ["go.mod"]
    },
    "tsserver": {
      "enable": false,
      "filetypes": ["js", "jsx", "ts", "tsx"],
      "module": "coc-tsserver"
    },
    "haskell": {
      "command": "haskell-language-server-8.10.3",
      "args": ["--lsp"],
      "rootPatterns": ["*.cabal", "stack.yaml", "cabal.project", "package.yaml", "hie.yaml"],
      "filetypes": ["haskell", "lhaskell"]
    },
    "zig": {
      "command": "zls",
      "filetypes": ["zig"]
    }
  }
}
