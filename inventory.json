{
  "dotfiles" : [
    {
      "name" : ".bashrc",
      "target" : "~/.bashrc",
      "platform" : "linux"
    },
    {
      "name" : "i3config",
      "target" : "~/.i3/config",
      "platform" : "linux"
    },
    {
      "name" : ".npmrc",
      "target" : "~/.npmrc",
      "platform" : "any"
    },
    {
      "name" : "tmux.conf",
      "target" : "/etc/tmux.conf",
      "platform" : "any",
      "owner" : "root"
    },
    {
      "name" : "vim-monokai/",
      "target" : "~/.vim/colors/vim-monokai",
      "platform" : "any"
    },
    {
      "name" : ".vimrc",
      "target" : "~/.vimrc",
      "platform" : "any"
    },
    {
      "name" : ".Xresources",
      "target" : "~/.Xresources",
      "platform" : "linux"
    }
  ]
}
